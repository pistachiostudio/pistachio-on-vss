{
    "ignorePatterns": [
      {
        "pattern": "^http://example.net"
      }
    ],
    "replacementPatterns": [
      {
        "pattern": "^.attachments",
        "replacement": "https://pistachiostudio.net/"
      },
      {
        "pattern": "^/",
        "replacement": "https://pistachiostudio.net/"
      }
    ],
    "httpHeaders": [
      {
        "urls": ["https://example.com"],
        "headers": {
          "Authorization": "Basic Zm9vOmJhcg==",
          "Foo": "Bar"
        }
      }
    ],
    "timeout": "20s",
    "retryOn429": true,
    "retryCount": 5,
    "fallbackRetryDelay": "30s",
    "aliveStatusCodes": [200, 206, 0, 403]
  }